    <script type="text/javascript">
      google.charts.load('current', {'packages':['corechart']});
      google.charts.setOnLoadCallback(drawChart);

      function drawChart() {

        var data = google.visualization.arrayToDataTable([
          ['Task', 'Hours per Day'],
          ['Full Stack',   <%-JSON.stringify(fullstack);%>],
          ['Unity',    <%-JSON.stringify(unity);%>],
          ['Mobile App',  <%-JSON.stringify(mobile);%>],
          ['Graphic',  <%-JSON.stringify(graphic);%>]
          
        ]);

        var options = {
          title: 'Number of Students in every departments',
           pieSliceText : 'value',
           tooltip:{isHtml:true}
        };

        var chart = new google.visualization.PieChart(document.getElementById('piechart'));
        google.visualization.events.addListener(chart, 'onmouseover', function(hover){
        if(hover){
            $('.google-visualization-tooltip-item:eq(1)').remove() // remove the other info
        }
    })
        chart.draw(data, options);
      }
      
    </script>
