                            <form action="/signUp" id="loginForm" method="POST" novalidate>
                                <div class="row">
                                    <div class="form-group col-lg-12">
                                        <label>Username</label>
                                        <input class="form-control" name="userName" value="<%=oldUserName%>">
                                    </div>
                                    <div class="form-group col-lg-6">
                                        <label>Password</label>
                                        <input type="password" class="form-control" name="password" value="<%=oldPassword%>">
                                    </div>
                                    <div class="form-group col-lg-6">
                                        <label>Repeat Password</label>
                                        <input type="password" class="form-control" name="repeatPassword"  value="<%=oldConfirmPassword%>">
                                    </div>
                                    <div class="form-group col-lg-6">
                                        <label>Email Address</label>
                                        <input class="form-control" name="email" type='email' value="<%=oldEmail%>">
                                    </div>
                                    <div class="form-group col-lg-6">
                                        <label>User's Type</label>
                                        <select class="form-control" name='type'  value="<%=oldType%>">
                                            <option>Staff</option>
                                            <option>Common User</option>
                                        </select>
                                       
                                    </div>
                                    <div class="form-group col-lg-6">
                                        <label>Department</label>
                                        <select class="form-control" name='dp'  value="<%=oldDepartment%>">
                                            <option>Full Stack</option>
                                            <option>Game</option>
                                            <option>Unity</option>
                                            <option>Mobile App</option>
                                        </select>
                                       
                                    </div>
